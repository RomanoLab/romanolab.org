{
  "name": "romanolab.org",
  "version": "0.1.0",
  "description": "Romano Lab Website",
  "main": "www/index.html",
  "scripts": {
    "watch:sass": "npx sass --watch src/_includes/stylesheets:docs/css",
    "build:sass": "npx sass src/_includes/stylesheets:docs/css",
    "watch:eleventy": "npx @11ty/eleventy --config=.eleventy.js --serve",
    "build:eleventy": "npx @11ty/eleventy --config=.eleventy.js",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "build-production": "ELEVENTY_PRODUCTION=true npm-run-all build*"
  },
  "author": "Joseph D. Romano",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "clean-css": "^5.3.0",
    "eleventy-sass": "^2.2.1",
    "luxon": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1"
  }
}
